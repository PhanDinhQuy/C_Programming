int lengthOfLongestSubstring(char* s) {
    int last[256];
    for(int i=0; i<256; i++)
    {
        last[i] = -1;
    }
    int max = 0;
    int start = 0;
    for(int i=0; s[i] != '\0'; i++)
    {
        unsigned char c = s[i];
        if (last[c] >= start)
        {
            start = last[c] + 1;
        }
        last[c] = i;

        int cur = i - start + 1;
        if(cur > max)
        {
            max = cur;
        }
    }
    return max;
}
